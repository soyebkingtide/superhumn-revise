/* min device off 990px */
@media screen and (min-width: 990px) {
  .header--middle-left {
    grid-template-columns: auto;
  }
  .header__inline-menu {
    text-align: center;
  }
  header.header {
    padding-top: 0.35rem;
    padding-bottom: 0.35rem;
  }
}


.text-behind-shadow {
  content: "";
  position: absolute;
  border-radius: 20px;
  background: #000;
  filter: blur(150px);
  display: block !important;
  max-width: 1191px;
  height: 630px;
  z-index: 1;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  width: 100%;
}

.team-feature-img {
  position: absolute;

  left: -100%;

  transition: all 1s;
  animation: myAnim 10s ease 0s infinite normal forwards;
  max-width: 240px;

}

.feature-img {
  position: absolute;

  right: -100%;

  transition: all 1s;
  animation: myAnim 10s ease 0s infinite normal forwards;
}


.px-0 {
  padding-left: 0 !important;
  padding-right: 0 !important;
}

.character {
  position: absolute;

  max-width: 450px;
  right: -100%;
  top: 100px;
  transition: all 1.8s;
  animation: myAnim 14s ease 0s infinite normal forwards;

}


@media(max-width: 992px) {

  .comeFromleft {
    left: 0 !important;
  }


  .character {


    right: -150px;
    top: 90px;


  }

  .feature-img {

    right: -10%;

  }

}

.character-from-right {

  right: -20px;

}


.comeFromleft {
  left: 0;
}

.comeFromRight {
  right: 0;
}

.fullvh {
  height: 100%;
  width: 100%;
}


@keyframes myAnim {
  0,
  100% {
    transform: translateY(0) rotate(0deg);
  }

  10%,
  30%,
  50%,
  70% {
    transform: translateY(-18px) rotate(2deg);
  }

  20%,
  40%,
  60% {
    transform: translateY(18px) rotate(0deg);
  }

  80% {
    transform: translateY(11.4px);
  }

  90% {
    transform: translateY(-11.4px) rotate(2deg);
  }
}


@media(max-width: 749px) {

  .mb-50 {
    margin-bottom: 3rem !important;
  }


  .btn-large {
    max-width: 315px;
  }


  .team-feature-img {


